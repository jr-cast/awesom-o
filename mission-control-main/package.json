{
  "name": "gravity",
  "plan": "mission-control",
  "version": "1.0.7",
  "description": "Manage users, view metrics, track events, collect feedback and log errors in one central dashboardy",
  "main": "server.js",
  "engines": {
    "node": "^16.14.2"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "chalk": "^4.1.2",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-rate-limit": "^6.4.0",
    "form-data": "^4.0.0",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "2.3.0",
    "mysql2": "2.3.3",
    "stripe": "9.11.0",
    "uuid": "^8.3.2",
    "mongodb": "4.12.1",
    "mongoose": "6.7.3",
    "express-mongo-sanitize": "2.2.0"
  },
  "devDependencies": {
    "concurrently": "^7.2.2",
    "nodemon": "^2.0.19"
  },
  "scripts": {
    "setup": "node bin/installcheck && npm install && npm link && cd client && npm install && cd ../ && npm run dev",
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "dev": "concurrently --kill-others \"npm run client\" \"npm run server\""
  },
  "nodemonConfig": {
    "ignore": [
      "client/",
      "config/",
      "server.js",
      "knexfile.js"
    ]
  },
  "repository": {
    "type": "git"
  },
  "author": "Kyle Gawley",
  "license": "PRIVATE",
  "private": true,
  "homepage": "https://usegravity.app"
}

{
  "name": "gravity",
  "plan": "server",
  "version": "9.0.19",
  "description": "Server application for Gravity",
  "main": "server.js",
  "engines": {
    "node": "^18.11.0"
  },
  "bin": {
    "gravity": "bin/gravity.js"
  },
  "dependencies": {
    "aws-sdk": "^2.1238.0",
    "axios": "^1.1.3",
    "bcrypt": "^5.1.0",
    "chalk": "^4.1.2",
    "config": "^3.3.8",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "cryptr": "^6.0.3",
    "dotenv": "^16.0.3",
    "expo-server-sdk": "^3.7.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.6.0",
    "form-data": "^4.0.0",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "2.3.0",
    "mongodb": "4.12.1",
    "mongoose": "6.7.3",
    "multer": "^1.4.4",
    "mysql2": "2.3.3",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-twitter": "^1.0.4",
    "qrcode": "^1.5.1",
    "randomstring": "^1.2.3",
    "speakeasy": "^2.0.0",
    "stripe": "10.15.0",
    "uuid": "^9.0.0",
    "express-mongo-sanitize": "2.2.0"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "concurrently": "^7.5.0",
    "mocha": "^10.1.0",
    "nodemon": "^2.0.20"
  },
  "scripts": {
    "setup": "node bin/installcheck && npm install && npm link; node bin/clientcheck && cd ../client && npm install; cd ../server && node bin/appcheck && cd ../app && npm install && cd ../server && node bin/setup.js; npm run dev",
    "client": "cd ../client && npm start",
    "server": "nodemon server.js",
    "app": "cd ../app && expo start",
    "dev": "node bin/start",
    "test": "mocha test/run --exit",
    "heroku-postbuild": "node bin/clientcheck && cd client && npm install && npm run build"
  },
  "nodemonConfig": {
    "ignore": [
      "client/",
      "config/",
      "server.js",
      "knexfile.js"
    ]
  },
  "repository": {
    "type": "git"
  },
  "author": "Kyle Gawley",
  "license": "PRIVATE",
  "private": true,
  "homepage": "https://usegravity.app"
}
